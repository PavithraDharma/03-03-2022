//String to char array conversion
import java.util.Scanner;
class Stringtochar {
    public static void main(String[] args) {
        String str;char arr[]=new char[15];
      Scanner scan=new Scanner(System.in);
      str=scan.nextLine();
      for(int i=0;i<str.length();i++)
      {
        arr[i]=str.charAt(i);
      }
      System.out.println("----Position Character----\n");
      for(int i=0;i<str.length();i++)
      {
       System.out.println(i+" "+arr[i]+"\n");
      }
    }
}
